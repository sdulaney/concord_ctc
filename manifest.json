{
  "update_url":"http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,

  "name": "Concord Click to Call",
  "description": "This is a custom extension to connect Propertybase with the Barracuda phone system.",
  "version": "1.3.1",
  "background": {
      "scripts": [ "phone-format.js", "app.js" ]
  },
  "browser_action": {
   "default_icon": "concord.png",
   "default_title": "CONCORD CLICK TO CALL",
   "default_popup": "popup.html"
  },
  "permissions" : [
    "tabs",
    "storage",
    "http://192.168.1.2/",
    "http://24.199.26.67/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [ "jquery-3.1.0.js", "underscore.js", "phone-format.js", "content.js", "vendor.js" ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "concord.png"
  },
  "web_accessible_resources": [ "/logo.png", "/concord.png", "/check_radio_sheet.png", "/popup.css", "/popup.js", "/app.js", "/content.js", "/jquery-3.1.0.js", "/phone-format.js", "/underscore.js", "/vendor.js" ]
}
